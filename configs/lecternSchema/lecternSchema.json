{
  "name": "clinical_data_dictionary",
  "description": "dictionary for describing clinical cancer patient data including demographics, diagnoses, specimens, treatments, and follow-ups",
  "version": "1.0",
  "schemas": [
    {
      "name": "patient",
      "description": "Clinical and demographic information about cancer patients",
      "fields": [
        {
          "meta": {
            "displayName": "Donor ID"
          },
          "name": "donor_id",
          "description": "Unique identifier for the patient/donor",
          "restrictions": {
            "required": true,
            "regex": "^DO\\d{4}$"
          },
          "valueType": "string",
          "unique": true
        },
        {
          "meta": {
            "displayName": "Gender"
          },
          "name": "gender",
          "description": "Patient's gender",
          "restrictions": {
            "required": true,
            "codeList": ["Male", "Female", "Other", "Unknown"]
          },
          "valueType": "string"
        },
        {
          "meta": {
            "displayName": "Vital Status"
          },
          "name": "vital_status",
          "description": "Current vital status of the patient",
          "restrictions": {
            "required": true,
            "codeList": ["Alive", "Deceased", "Unknown"]
          },
          "valueType": "string"
        }
      ]
    },
    {
      "name": "diagnosis",
      "description": "Cancer diagnosis information",
      "fields": [
        {
          "meta": {
            "displayName": "Donor ID"
          },
          "name": "donor_id",
          "description": "Reference to the patient/donor",
          "restrictions": {
            "required": true,
            "regex": "^DO\\d{4}$"
          },
          "valueType": "string"
        },
        {
          "meta": {
            "displayName": "Diagnosis ID"
          },
          "name": "diagnosis_id",
          "description": "Unique identifier for the diagnosis",
          "restrictions": {
            "required": true,
            "regex": "^PD\\d{6}$"
          },
          "valueType": "string",
          "unique": true
        },
        {
          "meta": {
            "displayName": "Primary Site"
          },
          "name": "primary_site",
          "description": "Primary site of the cancer",
          "valueType": "string",
          "restrictions": {
            "required": true
          }
        },
        {
          "meta": {
            "displayName": "Age at Diagnosis"
          },
          "name": "age_at_diagnosis",
          "description": "Patient's age at the time of diagnosis",
          "valueType": "integer",
          "restrictions": {
            "required": true,
            "range": {
              "min": 0,
              "max": 120
            }
          }
        },
        {
          "meta": {
            "displayName": "Cancer Type"
          },
          "name": "cancer_type",
          "description": "ICD-O-3 or ICD-10 cancer type code",
          "valueType": "string",
          "restrictions": {
            "required": true
          }
        },
        {
          "meta": {
            "displayName": "Staging System"
          },
          "name": "staging_system",
          "description": "Cancer staging system used",
          "valueType": "string"
        },
        {
          "meta": {
            "displayName": "Stage"
          },
          "name": "stage",
          "description": "Cancer stage according to the specified staging system",
          "valueType": "string"
        }
      ]
    },
    {
      "name": "specimen",
      "description": "Biological specimen information",
      "fields": [
        {
          "meta": {
            "displayName": "Donor ID"
          },
          "name": "donor_id",
          "description": "Reference to the patient/donor",
          "restrictions": {
            "required": true,
            "regex": "^DO\\d{4}$"
          },
          "valueType": "string"
        },
        {
          "meta": {
            "displayName": "Specimen ID"
          },
          "name": "specimen_id",
          "description": "Unique identifier for the specimen",
          "restrictions": {
            "required": true,
            "regex": "^SP\\d{6}$"
          },
          "valueType": "string",
          "unique": true
        },
        {
          "meta": {
            "displayName": "Specimen Type"
          },
          "name": "specimen_type",
          "description": "Type of specimen collected",
          "valueType": "string",
          "restrictions": {
            "required": true
          }
        },
        {
          "meta": {
            "displayName": "Tissue Source"
          },
          "name": "tissue_source",
          "description": "Source of the tissue specimen",
          "valueType": "string"
        },
        {
          "meta": {
            "displayName": "Sample Type"
          },
          "name": "sample_type",
          "description": "Type of sample derived from the specimen",
          "valueType": "string"
        }
      ]
    },
    {
      "name": "treatment",
      "description": "Treatment information",
      "fields": [
        {
          "meta": {
            "displayName": "Donor ID"
          },
          "name": "donor_id",
          "description": "Reference to the patient/donor",
          "restrictions": {
            "required": true,
            "regex": "^DO\\d{4}$"
          },
          "valueType": "string"
        },
        {
          "meta": {
            "displayName": "Treatment ID"
          },
          "name": "treatment_id",
          "description": "Unique identifier for the treatment",
          "restrictions": {
            "required": true,
            "regex": "^TR\\d{6}$"
          },
          "valueType": "string",
          "unique": true
        },
        {
          "meta": {
            "displayName": "Treatment Type"
          },
          "name": "treatment_type",
          "description": "Type of treatment administered",
          "valueType": "string",
          "restrictions": {
            "required": true
          }
        },
        {
          "meta": {
            "displayName": "Treatment Start"
          },
          "name": "treatment_start",
          "description": "Days from diagnosis to treatment start",
          "valueType": "integer",
          "restrictions": {
            "range": {
              "min": 0
            }
          }
        },
        {
          "meta": {
            "displayName": "Treatment Duration"
          },
          "name": "treatment_duration",
          "description": "Duration of treatment in days",
          "valueType": "integer",
          "restrictions": {
            "range": {
              "min": 0
            }
          }
        },
        {
          "meta": {
            "displayName": "Treatment Response"
          },
          "name": "treatment_response",
          "description": "Patient's response to the treatment",
          "valueType": "string"
        },
        {
          "meta": {
            "displayName": "Drug Name"
          },
          "name": "drug_name",
          "description": "Name of drug used in treatment",
          "valueType": "string"
        }
      ]
    },
    {
      "name": "followup",
      "description": "Follow-up assessment information",
      "fields": [
        {
          "meta": {
            "displayName": "Donor ID"
          },
          "name": "donor_id",
          "description": "Reference to the patient/donor",
          "restrictions": {
            "required": true,
            "regex": "^DO\\d{4}$"
          },
          "valueType": "string"
        },
        {
          "meta": {
            "displayName": "Follow-up ID"
          },
          "name": "followup_id",
          "description": "Unique identifier for the follow-up",
          "restrictions": {
            "required": true,
            "regex": "^FO\\d{6}$"
          },
          "valueType": "string",
          "unique": true
        },
        {
          "meta": {
            "displayName": "Follow-up Interval"
          },
          "name": "followup_interval",
          "description": "Days from diagnosis to follow-up",
          "valueType": "integer",
          "restrictions": {
            "range": {
              "min": 0
            }
          }
        },
        {
          "meta": {
            "displayName": "Disease Status"
          },
          "name": "disease_status",
          "description": "Status of the disease at follow-up",
          "valueType": "string"
        }
      ]
    }
  ]
}
